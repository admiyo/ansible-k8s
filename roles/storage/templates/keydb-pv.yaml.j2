apiVersion: v1
kind: PersistentVolume
metadata:
  name: keydbpv
  namespace: openstack
  annotations:
    volume.beta.kubernetes.io/storage-class: "slow"
  labels:
    app: keystone
spec:
  capacity:
    storage: 8Gi
  accessModes:
    - ReadWriteOnce
  persistentVolumeReclaimPolicy: Retain
  iscsi:
    targetPortal: 10.0.0.30:3260
    iqn: iqn.2009-02.com.younglogic:for.all
    lun: 1
    fsType: ext4
    readOnly: false